<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="frame_sign">
    <property name="height">200</property>
    <property name="padding">30</property>
    <property name="width">400</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="ttk.Button" id="btn_sign">
        <property name="command">open_win_main</property>
        <property name="cursor">target</property>
        <property name="state">normal</property>
        <property name="text" translatable="yes">Войти</property>
        <property name="width">88</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="inp_login">
        <property name="font">TkDefaultFont</property>
        <property name="takefocus">true</property>
        <property name="width">37</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="inp_pass">
        <property name="font">TkDefaultFont</property>
        <property name="show">•</property>
        <property name="width">37</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="lbl_login">
        <property name="text" translatable="yes">Логин</property>
        <layout>
          <property name="column">0</property>
          <property name="padx">100</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="lbl_pass">
        <property name="text" translatable="yes">Пароль</property>
        <layout>
          <property name="column">0</property>
          <property name="padx">100</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
